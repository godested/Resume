[*]
charset=utf-8
end_of_line=lf
insert_final_newline=false
indent_style=space
indent_size=2

[{*.ng,*.sht,*.html,*.shtm,*.shtml,*.htm}]
indent_style=space
indent_size=2

[{.babelrc,.stylelintrc,.eslintrc,jest.config,.browserslistrc,*.json,*.jsb3,*.jsb2,*.bowerrc}]
indent_style=space
indent_size=2

[*.css]
indent_style=space
indent_size=2
insert_final_newline=true

[{*.applejs,*.js}]
indent_style=space
indent_size=2
insert_final_newline=true

[*.jsx]
indent_style=space
indent_size=2
insert_final_newline=true

[*.scss]
indent_style=space
indent_size=2
insert_final_newline=true

[{.analysis_options,*.yml,*.yaml}]
indent_style=space
indent_size=2
