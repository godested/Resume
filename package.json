{
  "name": "resume",
  "private": true,
  "version": "0.1.0",
  "author": "godested@gmail.com",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/godested/resume.git"
  },
  "bugs": {
    "url": "https://github.com/godested/resume/issues"
  },
  "homepage": "https://github.com/godested/resume/#readme",
  "scripts": {
    "commit": "lint-staged && node ./node_modules/.bin/git-cz",
    "start": "react-app-rewired start",
    "build": "react-app-rewired build && node ./postbuild.js",
    "serve": "serve -s build -p 9090",
    "test": "react-app-rewired test",
    "styleguide": "styleguidist server"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.scss": [
      "node ./node_modules/.bin/stylelint --fix --syntax scss",
      "git add"
    ],
    "*.{jsx,js}": [
      "node ./node_modules/.bin/eslint --fix",
      "node ./node_modules/.bin/prettier --write",
      "git add"
    ],
    "*.{json,md}": [
      "node ./node_modules/.bin/prettier --write",
      "git add"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "dependencies": {
    "normalize.css": "^8.0.1",
    "react": "^16.10.2",
    "react-dom": "^16.10.2",
    "react-router-dom": "^5.1.2"
  },
  "devDependencies": {
    "@types/jest": "^24.0.15",
    "@types/react": "^16.8.23",
    "@types/react-dom": "^16.8.5",
    "@types/react-redux": "^7.1.4",
    "@types/react-router-dom": "^5.1.2",
    "@types/webpack-env": "^1.14.1",
    "@typescript-eslint/eslint-plugin": "^2.3.3",
    "@typescript-eslint/parser": "^2.3.3",
    "customize-cra": "^0.8.0",
    "eslint": "^6.6.0",
    "eslint-config-prettier": "^5.1.0",
    "eslint-import-resolver-typescript": "^2.0.0",
    "eslint-plugin-import": "2.18.2",
    "eslint-plugin-jsx-a11y": "6.2.3",
    "eslint-plugin-prettier": "^3.1.1",
    "eslint-plugin-react": "^7.16.0",
    "eslint-plugin-react-hooks": "^2.3.0",
    "git-cz": "^3.2.1",
    "husky": "^3.0.1",
    "jest": "^24.8.0",
    "lint-staged": "^9.2.1",
    "node-sass": "^4.13.0",
    "prettier": "^1.17.0",
    "react-app-rewired": "^2.1.1",
    "react-scripts": "3.2.0",
    "react-test-renderer": "^16.8.6",
    "stylelint": "^9.9.0",
    "stylelint-config-css-modules": "^1.3.0",
    "stylelint-config-standard": "^18.2.0",
    "stylelint-order": "^2.0.0",
    "stylelint-scss": "^3.4.4",
    "typescript": "^3.6.3"
  }
}
